package Abstraction;

public class Nested_Try {

	public static void main(String[] args) { 
		  try{
			  System.out.println("Execute");
			  try
			  {   
				  int b =39/0;
			  } 
			  
			  catch(ArrayIndexOutOfBoundsException e)  
			  {  
			    System.out.println(e);  
			  }
		      
		  }
		  catch(ArithmeticException e) {
			  System.out.println(e);
		  }

		  System.out.println("Outer try");
	}

}
